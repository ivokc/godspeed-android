<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent">
    <Button
        android:id="@+id/design_support_library"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="design support library"/>

    <Button
        android:id="@+id/tablayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="tablayout"/>

    <Button
        android:id="@+id/navigation_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="navigation view"/>

    <Button
        android:id="@+id/coordinatorlayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="coordinatorlayout"/>

    <Button
        android:id="@+id/collapsing_toolbar_activity"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="collapsing toolbar"/>



</LinearLayout>