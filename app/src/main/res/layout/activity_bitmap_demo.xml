<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent">

    <Button
        android:id="@+id/btn_BitmapStaticConstructor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="BitmapStaticConstructor"/>
    <Button
        android:id="@+id/btn_extra_alpha"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="extra_alpha"/>
    <Button
        android:id="@+id/btn_bitmap_density"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="bitmap_density"/>
    <Button
        android:id="@+id/btn_bitmap_pixel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="bitmap_pixel"/>

    <Button
        android:id="@+id/btn_bitmap_compress"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="bitmap_compress"/>
    <Button
        android:id="@+id/btn_bitmap_watermark"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="bitmap_watermark"/>

</LinearLayout>